#include<stdio.h>
int main()
{
    int n,i;
    printf("enter the size of the array:");
    scanf("%d",&n);
    int arr[n];
    printf("enter elements in array\n");
    for (i=0;i<n;i++) {
        scanf("%d",&arr[i]);
    }
    printf("negative number is :");
    for (i=0;i<n;i++){
        if (arr[i]<0)
        {
            printf("%d",arr[i]);
        }
    }
    return 0;
}
